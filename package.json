  "description": "",
  "main": "index.js",
  "scripts": {
    "bundle:analyze": "webpack --env mode=production --env analyze=true",
    "start": "webpack serve --env mode=development --env port=3000",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "format:src": "prettier --write \"src/**/*\"",
    "format:all": "prettier --write .",
    "lint:ts": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "lint:styles": "stylelint \"**/*.{scss,css}\"",
    "test:unit": "jest -c ./config/jest/jest.config.ts",
    "test:ui": "loki test",
    "test:ui:approve": "loki test approve",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build",
    "visual:report": "npm-run-all visual:report:*",
    "visual:report:json": "ts-node scripts/generate-visual-json-report.ts",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky"
  },
  "keywords": [],
